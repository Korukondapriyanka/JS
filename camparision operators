//  * Equality Operator (==)
//   The Equality operator is used to compare the equality of two operands.




// Illustration of (==) operator
let x = 3;
let y = '3';
​
// Checking of operands
console.log(x == 3);
console.log(y == 3);        
console.log(x == y);
​
console.log();
​
// Check against special values
console.log(NaN == NaN);
console.log(0 == false);   
console.log(0 == null);
